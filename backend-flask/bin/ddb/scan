#! /usr/bin/env python3

import boto3
import sys
from datetime import datetime, timedelta, timezone
import uuid
import os

attrs = {
  'endpoint_url': 'http://localhost:8000'
}

ddb = boto3.resource('dynamodb',**attrs)
table_name='cruddur-message'

table = ddb.Table(table_name)
response = table.scan()

print('Response====')
print(response)

items = response['Items']
for item in items:
    print(item)


